    <validationRules>
        <fullName>Attendee_Assigned_With_Future_End_Date</fullName>
        <active>true</active>
        <description>Attendee can only be associated with the Event whose End Date is in future.</description>
        <errorConditionFormula>AND(  DATEVALUE( Event_for_Pro_FIT__r.End__c  ) &lt; (DATEVALUE(  Event_for_Pro_FIT__r.Start__c ) + 1), NOT (ISNULL(Event_for_Pro_FIT__r.End__c)  ) )</errorConditionFormula>
        <errorDisplayField>Event_for_Pro_FIT__c</errorDisplayField>
        <errorMessage>End Date/Time of the Event must be at-least 1 day ahead of Start Date/Time</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Event_Live_Is_Checked</fullName>
        <active>true</active>
        <description>Event Live should be checked.</description>
        <errorConditionFormula>Event_for_Pro_FIT__r.Live__c = FALSE</errorConditionFormula>
        <errorDisplayField>Event_for_Pro_FIT__c</errorDisplayField>
        <errorMessage>The event that you selected is not Live. Please select Live Event.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>If_Remaining_Seat_Is_Not_Zero</fullName>
        <active>true</active>
        <description>The event should have available seat.</description>
        <errorConditionFormula>Event_for_Pro_FIT__r.Remaining_Seats__c   &lt;=  0</errorConditionFormula>
        <errorDisplayField>Event_for_Pro_FIT__c</errorDisplayField>
        <errorMessage>The event that you selected is already full. Please make sure that event still have available seat.</errorMessage>
    </validationRules>
