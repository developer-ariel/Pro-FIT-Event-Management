    <validationRules>
        <fullName>End_date_must_be_1_day_ahead_of_Startdat</fullName>
        <active>true</active>
        <description>End Date/Time must be at-least 1 day ahead of Start Date/Time ( If there is a value in End Date/Time field )</description>
        <errorConditionFormula>AND(  DATEVALUE(End__c ) &lt; (DATEVALUE( Start__c ) + 1), NOT (ISNULL(End__c)  ) )</errorConditionFormula>
        <errorDisplayField>End__c</errorDisplayField>
        <errorMessage>End Date/Time must be at-least 1 day ahead of Start Date/Time</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>If_Event_type_In_Person_Fill_Location</fullName>
        <active>true</active>
        <description>If event type is In-Person, User must select location.</description>
        <errorConditionFormula>AND(ISBLANK( Location__c ), ISPICKVAL( Event_Type__c ,&quot;In-Person&quot;))</errorConditionFormula>
        <errorDisplayField>Location__c</errorDisplayField>
        <errorMessage>You must select Location if the Event-type is In-Person.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>If_Location_Active_Event_Type_In_person</fullName>
        <active>true</active>
        <description>If there is a Location, the event type is mandatory and should be In-Person</description>
        <errorConditionFormula>AND(NOT(ISBLANK( Location__c )), ISPICKVAL( Event_Type__c ,&apos;&apos;))</errorConditionFormula>
        <errorDisplayField>Location__c</errorDisplayField>
        <errorMessage>You must select Event type as In-Person if you want to add location.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>If_Virtual_Prevent_User_To_Select_Locati</fullName>
        <active>true</active>
        <description>If event type is Virtual, prevent User to select location.</description>
        <errorConditionFormula>AND(NOT( ISBLANK( Location__c ) ), ISPICKVAL( Event_Type__c  ,&quot;Virtual&quot;))</errorConditionFormula>
        <errorDisplayField>Location__c</errorDisplayField>
        <errorMessage>You are not allowed to select location if the event type is Virtual.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Recurring_Checked_Fill_Frequency_Field</fullName>
        <active>true</active>
        <description>If Recurring? checkbox is checked then user must need to fill Frequency field.</description>
        <errorConditionFormula>AND(
   Reccurring__c = TRUE,
  OR(
  ISPICKVAL( Frequency__c , &quot;&quot;)
 )
)</errorConditionFormula>
        <errorDisplayField>Frequency__c</errorDisplayField>
        <errorMessage>If Recurring is checked, you must select Frequency type.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Recurring_UnChecked_Dont_Fill_Frequency</fullName>
        <active>true</active>
        <description>If Recurring? checkbox is unchecked, User can not select Frequency field.</description>
        <errorConditionFormula>AND(
 Reccurring__c = FALSE,
 OR(
 NOT( ISBLANK( TEXT( Frequency__c ) ) )
)
)</errorConditionFormula>
        <errorDisplayField>Frequency__c</errorDisplayField>
        <errorMessage>You are not allowed to select Frequency type if Recurring is not checked.</errorMessage>
    </validationRules>
